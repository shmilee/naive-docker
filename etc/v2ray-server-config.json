{
  "log" : {
    "access": "/srv/log/v2ray-access.log",
    "error": "/srv/log/v2ray-error.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": {{port1}}, // 服务器监听端口
      "protocol": "vmess",    // 主传入协议
      "settings": {
        "clients": [
          {
            "id": "{{UUID1}}",  // 用户 ID，客户端与服务器必须相同
            "alterId": 64
          }
        ]
      }
    },
    {
      "port": 44944,
      "listen": "127.0.0.1",
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "{{UUID2}}",
            "alterId": 64
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
        "path": "/{{v2raypath}}"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",  // 主传出协议
      "settings": {}
    }
  ]
}
